      <DTS:Variables />
      <DTS:PropertyExpression
          DTS:Name="SqlStatementSource">"INSERT INTO [DBO].[DFT_EMPSUMMARY] select a.[DEPTID], count(*) AS TOTAL_EMP
FROM [DBO].[DFT_EMPLOYEES_DIM] a GROUP BY a.[DEPTID]"
      </DTS:PropertyExpression>
      <DTS:ObjectData>
          <SQLTask:SqlTaskData
            SQLTask:Connection="{83863046-F4AB-407C-83AF-5BE7C039B488}"
            SQLTask:SqlStatementSource="USE [nvz_profiler]&#xA; DELETE FROM [DBO].[DFT_EMPSUMMARY]&#xA; INSERT INTO [DBO].[DFT_EMPSUMMARY] select a.[DEPTID], count(*) AS TOTALEMP &#xA; FROM DFT_EMPLOYEES_DIM a GROUP BY a.[DEPTID]"
