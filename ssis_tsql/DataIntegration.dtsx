      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Load DeptSummary Data"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
          <SQLTask:SqlTaskData
            SQLTask:Connection="{83863046-F4AB-407C-83AF-5BE7C039B488}"
            SQLTask:SqlStatementSource="USE [nvz_profiler]&#xA; DELETE FROM [DBO].[DFT_DEPTSUMMARY]&#xA; INSERT INTO [DBO].[DFT_DEPTSUMMARY] with managers as (select empid, empname from DFT_EMPLOYEES) select a.[DEPTNAME], b.[TOTALEMP], c.empname FROM DFT_DEPARTMENTS a &#xA; INNER JOIN DFT_EMPSUMMARY b ON a.[DEPTID] = b.[DEPTID] &#xA; inner join managers c on a.managerid = c.empid"
            SQLTask:ResultType="ResultSetType_Rowset" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
             SQLTask:ResultName="0"
              SQLTask:DtsVariableName="User::Triggered_Task_Names" />
